{
  "name": "serverless-employee",
  "version": "1.0.0",
  "description": "This is simple REST API CRUD for AWS Lambda By Serverless framework with TypeScript and MongoDB Atlas.",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "local": "serverless offline",
    "deploy": "serverless deploy",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "dotenv": "^8.2.0",
    "mongoose": "^5.9.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.51",
    "@types/dotenv-safe": "^8.1.0",
    "@types/jest": "^26.0.23",
    "@types/mongoose": "^5.7.14",
    "babel-jest": "^26.6.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "jest": "^26.6.3",
    "serverless-offline": "^6.9.0",
    "serverless-plugin-typescript": "^1.1.9",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.9.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ezequiassam/serverless-employee.git"
  },
  "keywords": [
    "Nodejs",
    "TypeScript",
    "ServerLess",
    "MongoDB Atlas",
    "AWS Lambda",
    "Jest"
  ],
  "author": "Ezequias Sampaio",
  "license": "ISC",
  "homepage": "https://github.com/ezequiassam/serverless-employee#readme",
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**"
    ],
    "testURL": "http://localhost/"
  }
}
